{
  "name": "platform-backoffice",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "serve-en": "ng serve --configuration=en --port=4201",
    "serve-en2": "ng serve --configuration=en --port=4202",
    "build": "ng build --configuration=production --localize",
    "test": "ng test",
    "hooks:pre-commit": "npm-run-all prettier:fix-staged",
    "hooks:pre-push": "npm-run-all lint",
    "prettier:fix-staged": "pretty-quick --staged",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm-run-all hooks:pre-commit"
    }
  },
